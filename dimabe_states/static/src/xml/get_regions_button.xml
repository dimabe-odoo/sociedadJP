<template xml:space="preserve">
    <t t-extend="ListView.buttons">
      <t t-jquery="button.o_list_button_add" t-operation="after">
        <button t-if="widget.modelName == 'custom.region'" id="get_regions" class="btn btn-primary oe_action_button"
                type="button" accesskey="f">Obtener Regiones</button>
          <button t-if="widget.modelName == 'custom.province'" id="get_province" class="btn btn-primary oe_action_button"
                  type="button" accesskey="f">Obtener Provincias</button>
          <button t-if="widget.modelName == 'custom.commune'" id="get_commune" class="btn btn-primary oe_action_button"
                  type="button" accesskey="f">Obtener Comunas</button>
      </t>
    </t>
  </template>