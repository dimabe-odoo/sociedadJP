<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="custom_regions_cron" model="ir.cron">
            <field name="name">Obtener Regiones (CL)</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_custom_region"/>
            <field name="state">code</field>
            <field name="code">model.get_regions()</field>
        </record>
        <record id="custom_province_cron" model="ir.cron">
            <field name="name">Obtener Provincias (CL)</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_custom_province"/>
            <field name="state">code</field>
            <field name="code">model.get_provinces()</field>
        </record>
        <record id="custom_commune_cron" model="ir.cron">
            <field name="name">Obtener Comuna (CL)</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_custom_commune"/>
            <field name="state">code</field>
            <field name="code">model.get_communes()</field>
        </record>
    </data>
</odoo>