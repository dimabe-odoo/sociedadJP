<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_view_picking_form_inherit" model="ir.ui.view">
            <field name="name">Stock Picking</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="supply_dispatch_id" widget="many2onebutton"
                           attrs="{'visible':['&',('picking_type_code','!=','outgoing'),('purchase_without_supply', '=', False)]}"/>
                </xpath>
                <xpath expr="//field[@name='location_dest_id']" position="after">
                    <field name="purchase_without_supply" attrs="{'visible':[('picking_type_code','!=','outgoing')]}" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>