<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="purchase_purchase_order_form_inherit" model="ir.ui.view">
            <field name="name">purchase_purchase_order_form_inherit</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="priority" eval="1000"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='order_line']//tree//field[@name='product_qty']" position="after">
                    <field name="purchase_without_supply" attrs="{'invisible':[('parent.have_purchase_without_supply','=',False)]}"/>
                </xpath>
                <xpath expr="//field[@name='date_order']" position="after">
                    <field name="have_purchase_without_supply"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>